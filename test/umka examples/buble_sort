fun sort(arr) -> [] {
    let n = len(arr);
    for (let i = 0; i < n; i = i + 1) {
        for (let j = 0; j < n - i - 1; j = j + 1) {
            if (get(arr, j) > get(arr, j + 1)) {
                let temp = get(arr, j);
                set(arr, j, get(arr, j + 1));
                set(arr, j + 1, temp);
            }
        }
    }
    return arr;
}

fun main() -> unit {
    let arr = [3, 1, 4, 2];
    let sorted = sort(arr);
    print(sorted);
    return;
}