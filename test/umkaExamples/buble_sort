fun sort(arr) -> unit {
    let n = len(arr);
    for (let i = 0; i < n; i = i + 1) {
        for (let j = 0; j < n - i - 1; j = j + 1) {
            if (get(arr, j) > get(arr, j + 1)) {
                let temp = get(arr, j);
                set(arr, j, get(arr, j + 1));
                set(arr, j + 1, temp);
            }
        }
    }
}

fun main() -> unit {
    let arr = [3, 1, 4, 2, 0.44, true];
    sort(arr);
    print(arr);
    return;
}